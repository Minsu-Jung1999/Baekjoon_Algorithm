#include <stdio.h>
//#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#define _CRT_SECURE_NO_WARNINGS
using namespace std;

int main()
{
	int r;
	long long num1;
	long long num2;
	scanf("%d", &r);
	for (int i = 0; i < r; i++)
	{
		scanf("%lld %lld", &num1, &num2);
		printf("%lld \n",num1 + num2);
	}

}
