#include <iostream>
#include <cstring>
#pragma warning(disable:4996)
using namespace std;


/**

3	4 16 41 85 155	.... 12 25 44 70
2	2 07 16 30 50	.... 05 09 16 20
1	1 03 06 10 15	.... 02 03 04 05
0	1 02 03 04 05
*/

int main()
{
	int n, h, l, total = 0;
	cin >> n;
	while (n>0)
	{
		cin >> l;
		cin >> h;
		for (int i = 0; i < l-1; i++)
		{
			for (int j = 1; j < h; j++)
			{
				total += h;
			}
		}

		n--;
	}
}

