//#include <stdio.h>
#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <numeric>
#include <stack>
#define _CRT_SECURE_NO_WARNINGS
using namespace std;


// 펠린드롬  65 = A
int main()
{

	int size;
	string arr;
	int odd_num=0;
	int a_pre[26] = {0};
	char pelind[51];
	cin >> arr;
	size = arr.size();
	int fr = 0;
	int r = size-1;
	for (int i = 0; i < size; i++)
	{
		a_pre[(arr[i] - '0')-17]++;
	}
	for (int i = 0; i < size; i++)
	{
		if (a_pre[i] != 0 && a_pre[i] % 2 == 1) odd_num++;
	}
	
	if (size % 2 == 0 && odd_num > 0) cout << "I'm Sorry Hansoo" << endl;
	else if (size % 2 == 1 && odd_num > 1) cout << "I'm Sorry Hansoo" << endl;

	for (int i = 0; i < 26; i++)
	{
		for (int j = 0; j < a_pre[i]; j++)
		{

			if (a_pre[i] %2==0)
			{
				pelind[fr++] = (i + '0')+17;
				pelind[r--] = (i + '0')+17;
			}
			else
			{
				pelind[size / 2] = (i + '0')+17;
			}
		}
	}
	cout << pelind;
}
