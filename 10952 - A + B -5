//#include <stdio.h>
#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <numeric>
#define _CRT_SECURE_NO_WARNINGS
using namespace std;

int main()
{
	int a;
	int b;
	while (true)
	{
		cin >> a >> b;
		if (a == 0 && b == 0)
			break;
		cout << a + b << endl;
		
	}
	
}

