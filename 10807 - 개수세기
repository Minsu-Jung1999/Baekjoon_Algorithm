//#include <stdio.h>
#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <numeric>
#define _CRT_SECURE_NO_WARNINGS
using namespace std;

int main()
{
	int b=0;
	int a;
	int arr[101];
	cin >> a;
	for (int i = 0; i < a; i++)
	{
		cin >> b;
		arr[i] = b;
	}
	cin >> b;
	int s = 0;
	for (int i = 0; i < a; i++)
	{
		if (arr[i] == b)
			s++;
	}
	cout << s;
}

