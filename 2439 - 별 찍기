//#include <stdio.h>
#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
//#define _CRT_SECURE_NO_WARNINGS
using namespace std;

int main()
{
	int a,b=0;
	cin >> a;
	for (int i = 0; i < a; i++)
	{
		for (int j = 0; j < a; j++)
		{
			if (j >= a - i - 1) {
				cout << "*";
			}
			else
				cout << " ";
		}
		cout << endl;
	}

}

